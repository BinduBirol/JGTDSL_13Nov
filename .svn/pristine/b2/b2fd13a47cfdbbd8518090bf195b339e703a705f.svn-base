package org.jgtdsl.dto;

public class MCollDueDTO {

	private String customer_id;
	private String customer_name;
	private double dues_on_prev_month;
	private double sales_this;
	private double collection_this;
	private double dues_on_this_month;
	private double avrg_sales;
	private double due_months;
	
	private String ministry_name;

	public String getCustomer_id() {
		return customer_id;
	}

	public void setCustomer_id(String customer_id) {
		this.customer_id = customer_id;
	}

	public String getCustomer_name() {
		return customer_name;
	}

	public void setCustomer_name(String customer_name) {
		this.customer_name = customer_name;
	}

	public double getDues_on_prev_month() {
		return dues_on_prev_month;
	}

	public void setDues_on_prev_month(double dues_on_prev_month) {
		this.dues_on_prev_month = dues_on_prev_month;
	}

	public double getSales_this() {
		return sales_this;
	}

	public void setSales_this(double sales_this) {
		this.sales_this = sales_this;
	}

	public double getCollection_this() {
		return collection_this;
	}

	public void setCollection_this(double collection_this) {
		this.collection_this = collection_this;
	}

	public double getDues_on_this_month() {
		return dues_on_this_month;
	}

	public void setDues_on_this_month(double dues_on_this_month) {
		this.dues_on_this_month = dues_on_this_month;
	}

	public double getAvrg_sales() {
		return avrg_sales;
	}

	public void setAvrg_sales(double avrg_sales) {
		this.avrg_sales = avrg_sales;
	}

	public double getDue_months() {
		return due_months;
	}

	public void setDue_months(double due_months) {
		this.due_months = due_months;
	}

	public String getMinistry_name() {
		return ministry_name;
	}

	public void setMinistry_name(String ministry_name) {
		this.ministry_name = ministry_name;
	}
	
	
}
